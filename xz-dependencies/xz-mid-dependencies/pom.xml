                            <artifactId>did-client</artifactId>
                            <version>${did.version}</version>
                        </dependency>-->
            <!--<dependency>
                <groupId>com.shulidata.commons</groupId>
                <artifactId>common-security-jwt</artifactId>
                <version>${sl-commons-jwt.version}</version>
            </dependency>-->
            <dependency>
                <groupId>com.aliyun.oss</groupId>
