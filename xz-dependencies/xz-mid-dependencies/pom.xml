        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <xz-commons-base.version>1.0.0-SNAPSHOT</xz-commons-base.version>
        <fastjson.version>1.2.70</fastjson.version>
        <!--配置中心版本-->
